<% include partials/header %>
<div class="container
  <div class="row">
    <h1 style="text-align: center;">Create a New campground</h1>
    <div style="width: 30%; margin: 0 auto;">
      <form class="form" action="/campgrounds" method="post">
        <div class="form-group">
          <input class="form-control" type="text" name="name" placeholder="Name" />
        </div>
        <div class="form-group">
          <input class="form-control" type="text" name="imageURL" placeholder="Image Url">
        </div>
        <div class="form-group">
          <button class="btn btn-large btn-primary btn-block" type="submit" name="button">Submit</button>
        </div>



      </form>
    </div>
  </div>
</div>

<% include partials/footer %>
